{
  "name": "massimino",
  "version": "0.1.0",
  "description": "The safety-first fitness community platform for trainers and enthusiasts",
  "private": true,
  "scripts": {
    "clean": "bash -c 'rm -rf .next .next-dev.log .next-dev.pid tsconfig.tsbuildinfo'",
    "predev": "npm run clean",
    "prebuild": "npm run clean",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "safety:check": "npm run lint && npm run type-check && npm run test",
    "deploy:build": "npm run build && npm run db:generate",
    "deploy:full": "npm run deploy:build && npm run db:migrate:prod && npm run start:prod",
    "scripts:exercises:import:local": "ts-node scripts/import-exercises-local.ts",
    "scripts:exercises:import": "ts-node scripts/import-exercises.ts",
    "scripts:exercises:sync:firestore": "ts-node -e \"(async()=>{const { prisma } = require('./dist')?.lib?.database?.client || require('./src/lib/database/client'); const { publishExercise } = require('./src/lib/integrations/firebase'); const all = await prisma.exercise.findMany(); let n=0; for (const ex of all){ await publishExercise(ex); if(++n%200===0) console.log('...synced', n);} console.log('done'); process.exit(0)})()\""
  },
  "dependencies": {
    "@mollie/api-client": "^4.3.3",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/components": "^0.5.5",
    "@react-email/head": "^0.0.12",
    "@react-email/html": "^0.0.11",
    "@react-email/preview": "^0.0.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/csv-parse": "^1.1.12",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "firebase-admin": "^13.5.0",
    "lucide-react": "^0.294.0",
    "next": "^15.0.0",
    "next-auth": "^4.24.11",
    "openai": "^4.20.1",
    "react": "^18.2.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "^18.2.0",
    "react-email": "^4.2.12",
    "react-hook-form": "^7.48.2",
    "resend": "^6.1.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "prisma": "^6.16.0",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "fitness",
    "safety",
    "community",
    "personal-training",
    "moderation"
  ]
}
