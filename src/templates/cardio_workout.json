        {
          "exercise_id": "warmup-walk",
          "exercise_name": "Warm-Up Walk",
          "order": 1,
          "sets": 1,
          "reps": "5 min",
          "rest_seconds": 0,
          "notes": "Easy pace, prepare body for movement",
          "primary_muscle_groups": ["cardiovascular", "legs"],
          "equipment": ["bodyweight"]
        },
        {
          "exercise_id": "jog-interval",
          "exercise_name": "Jog Interval",
          "order": 2,
          "sets": 7,
          "reps": "1 min",
          "rest_seconds": 0,
          "notes": "Maintain nasal breathing (Zone 2)",
          "primary_muscle_groups": ["cardiovascular", "quadriceps", "calves"],
          "equipment": ["bodyweight"]
        },
        {
          "exercise_id": "brisk-walk",
          "exercise_name": "Brisk Walk Recovery",
          "order": 3,
          "sets": 7,
          "reps": "2 min",
          "rest_seconds": 0,
          "notes": "Active recovery between jog intervals",
          "primary_muscle_groups": ["cardiovascular", "legs"],
          "equipment": ["bodyweight"]
