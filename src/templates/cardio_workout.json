  "version": "2.0_standardized",
  "duration_weeks": 3,
  "session_duration": {
    "min_minutes": 30,
    "max_minutes": 60
  },
  "frequency_per_week": 4,
  "level": "Beginner to Intermediate",
  "goal": {
    "outcome_goal": "Jog 5 km in under 35 min by end of Week 3",
