          },
          {
            "exercise": "Russian Twist",
            "sets": "3",
            "reps": "20 total",
            "tempo": "Controlled rotation",
            "rest": "30s",
            "notes": "Light or no weight. Oblique control for turns.",
            "imageUrl": "/exercises/russian-twist/0.jpg"
          },
          {
            "exercise": "Plank",
            "sets": "3",
            "reps": "30 sec each side (side plank)",
            "tempo": "Hold",
            "rest": "30s",
            "notes": "Side planks for lateral stability",
            "imageUrl": "/exercises/plank/0.jpg"
          }
        ]
      },
      {
        "section": "upper_body",
        "description": "Toned arms for port de bras and partnering",
        "exercises": [
          {
            "exercise": "Lat Pulldown",
            "sets": "3",
            "reps": "15",
            "tempo": "Controlled, squeeze",
            "rest": "45s",
            "notes": "Wide grip - develops beautiful back lines",
            "imageUrl": "/exercises/lat-pulldown/0.jpg"
          },
          {
            "exercise": "Dumbbell Bench Press",
            "sets": "3",
            "reps": "15",
            "tempo": "Controlled",
            "rest": "45s",
            "notes": "Light weights - strength without bulk",
            "imageUrl": "/exercises/dumbbell-bench-press/0.jpg"
          },
          {
            "exercise": "Lateral Raise",
            "sets": "3",
            "reps": "15",
            "tempo": "Slow raise, slow lower",
            "rest": "30s",
            "notes": "Very light weights. Develops second position arm control.",
            "imageUrl": "/exercises/lateral-raise/0.jpg"
          },
          {
            "exercise": "Face Pull",
            "sets": "3",
            "reps": "15",
            "tempo": "Squeeze at end",
            "rest": "30s",
            "notes": "Posture and shoulder health - prevents rounded shoulders",
            "imageUrl": "/exercises/face-pull/0.jpg"
          },
          {
            "exercise": "Dumbbell Curl",
            "sets": "3",
            "reps": "15",
            "tempo": "Controlled",
            "rest": "30s",
            "notes": "Light weights for toned arms without bulk",
            "imageUrl": "/exercises/dumbbell-curl/0.jpg"
          },
          {
            "exercise": "Tricep Pushdown",
            "sets": "3",
            "reps": "15",
            "tempo": "Squeeze at bottom",
            "rest": "30s",
            "notes": "Defined, toned triceps",
            "imageUrl": "/exercises/tricep-pushdown/0.jpg"
          }
        ]
      }
    ]
  },

