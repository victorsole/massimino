{
  "program": "The GGF Fitness Workout for Ballet Dancers",
  "author": "Gabriel Garcia Forn",
  "era": "Contemporary Ballet Era",
  "version": "1.0",
  "program_id": "ggf-ballet-fitness",
  "target_audience": "Professional ballet dancers, dance students, and those seeking dancer-like physique and control",
  "duration_weeks": 8,
  "session_duration_minutes": {
    "min": 45,
    "max": 60
  },
  "frequency_per_week": 4,
  "rest_days_per_week": 3,
  "level": "Intermediate",
  "programType": "ATHLETE",
  "settings": ["Gym with cable machines", "Mirror recommended for form check"],
  "equipment": {
    "required": ["Dumbbells (light to moderate)", "Cable machine", "Resistance bands", "Mat"],
    "recommended": ["Ballet barre or sturdy chair", "Foam roller", "Stability ball"],
    "optional": ["Ankle weights", "Pilates ring"]
  },

  "athlete_info": {
    "name": "Gabriel Garcia Forn",
    "discipline": "Professional Ballet",
    "training_philosophy": "Strength without bulk, control through full range of motion, injury prevention as priority"
  },

  "program_philosophy": {
    "origin": "This program combines classical ballet conditioning principles with modern sports science. Professional dancers need exceptional strength-to-weight ratio, impeccable body control, and bulletproof joints to perform demanding choreography night after night.",
    "ggf_quote": "\"Dance is controlled falling. Every movement requires strength, balance, and trust in your body. The gym builds the foundation that lets artistry flourish.\"",
    "core_principles": [
      "Strength without excessive muscle bulk - high reps, moderate weight",
      "Full range of motion in every exercise - mimics dance demands",
      "Single-leg work for balance and symmetry",
      "Core control in every movement - the center of all dance",
      "Flexibility maintenance alongside strength building",
      "Foot and ankle strength for pointe work and jumps"
    ]
  },

  "realistic_expectations": {
    "8_week_realistic_outcomes": [
      "Improved jump height and landing control",
      "Stronger, more stable ankles and feet",
      "Enhanced core endurance for sustained positions",
      "Better balance on single leg (retire, arabesque)",
      "Increased turnout control and hip strength",
      "Reduced risk of common dance injuries"
    ],
    "timeline": {
      "weeks_1_2": "Foundation - neuromuscular activation and movement quality",
      "weeks_3_4": "Building strength endurance",
      "weeks_5_6": "Power and control integration",
      "weeks_7_8": "Performance readiness and refinement"
    }
  },

  "training_schedule": {
    "day_1": "Lower Body Strength & Turnout",
    "day_2": "Core & Upper Body Control",
    "day_3": "Rest / Flexibility & Recovery",
    "day_4": "Single-Leg & Balance Focus",
    "day_5": "Rest / Light Cardio or Barre",
    "day_6": "Full Body Integration & Power",
    "day_7": "Rest / Active Recovery"
  },

  "phase_structure": [
    {
      "phase": "Activation & Foundation",
      "weeks": [1, 2],
      "focus": "Wake up stabilizers, establish movement patterns",
      "intensity": "Light to moderate",
      "rep_range": "12-15",
      "sets": "3",
      "rest_seconds": "45-60",
      "key_principle": "Mind-muscle connection"
    },
    {
      "phase": "Strength Endurance",
      "weeks": [3, 4, 5],
      "focus": "Build muscular endurance for long rehearsals and performances",
      "intensity": "Moderate - higher reps",
      "rep_range": "15-20",
      "sets": "3",
      "rest_seconds": "30-45",
      "key_principle": "Sustained control under fatigue"
    },
    {
      "phase": "Performance Peak",
      "weeks": [6, 7, 8],
      "focus": "Power, control, and movement quality",
      "intensity": "Moderate to high with focus on quality",
      "rep_range": "10-12",
      "sets": "3",
      "rest_seconds": "45-60",
      "key_principle": "Elegance meets strength"
    }
  ],

  "day_1_lower_body_turnout": {
    "focus": "Lower Body Strength & Turnout",
    "goal": "Build leg strength while maintaining dancer lines and improving turnout",
    "sections": [
      {
        "section": "warm_up",
        "description": "Hip and leg activation",
        "exercises": [
          {
            "exercise": "Hip Flexor Stretch",
            "sets": "2",
            "reps": "45 sec each side",
            "tempo": "Hold with breath",
            "rest": "None",
            "notes": "Essential for dancer hip mobility - deep lunge position",
            "imageUrl": "/exercises/hip-flexor-stretch/0.jpg"
          },
          {
            "exercise": "Leg Extension",
            "sets": "2",
            "reps": "20",
            "tempo": "Slow, squeeze at top",
            "rest": "30s",
            "notes": "Light weight - VMO activation for knee health",
            "imageUrl": "/exercises/leg-extension/0.jpg"
          },
          {
            "exercise": "Lying Leg Curl",
            "sets": "2",
            "reps": "15",
            "tempo": "Controlled",
            "rest": "30s",
            "notes": "Hamstring activation",
            "imageUrl": "/exercises/lying-leg-curl/0.jpg"
          }
        ]
      },
      {
        "section": "lower_body_strength",
        "description": "Building dancer-strong legs",
        "exercises": [
          {
            "exercise": "Bulgarian Split Squat",
            "sets": "3",
            "reps": "15 each leg",
            "tempo": "3 sec down, controlled up",
            "rest": "45s each leg",
            "notes": "Light dumbbells. Focus on turnout and alignment. Keep hips square.",
            "imageUrl": "/exercises/bulgarian-split-squat/0.jpg"
          },
          {
            "exercise": "Romanian Deadlift",
            "sets": "3",
            "reps": "12",
            "tempo": "Slow eccentric",
            "rest": "60s",
            "notes": "Light to moderate weight. Feel the hamstring stretch - critical for arabesques",
            "imageUrl": "/exercises/romanian-deadlift/0.jpg"
          },
          {
            "exercise": "Leg Press",
            "sets": "3",
            "reps": "20",
            "tempo": "Controlled",
            "rest": "60s",
            "notes": "Feet in turnout position (first position). Higher reps for endurance without bulk.",
            "imageUrl": "/exercises/leg-press/0.jpg"
          },
          {
            "exercise": "Lunge",
            "sets": "3",
            "reps": "12 each leg",
            "tempo": "Slow and controlled",
            "rest": "45s",
            "notes": "Reverse lunges - maintain turnout throughout. No weights or light dumbbells.",
            "imageUrl": "/exercises/lunge/0.jpg"
          }
        ]
      },
      {
        "section": "ankle_foot_strength",
        "description": "Critical for pointe work and jumps",
        "exercises": [
          {
            "exercise": "Standing Calf Raise",
            "sets": "4",
            "reps": "20",
            "tempo": "2 sec hold at top (releve)",
            "rest": "30s",
            "notes": "Both parallel and turnout positions. Full releve at top.",
            "imageUrl": "/exercises/standing-calf-raise/0.jpg"
          },
          {
            "exercise": "Seated Calf Raise",
            "sets": "3",
            "reps": "20",
            "tempo": "Slow, full ROM",
            "rest": "30s",
            "notes": "Soleus strength for demi-plie stability",
            "imageUrl": "/exercises/seated-calf-raise/0.jpg"
          }
        ]
      },
      {
        "section": "core_finish",
        "description": "Core activation to close",
        "exercises": [
          {
            "exercise": "Plank",
            "sets": "2",
            "reps": "45 sec",
            "tempo": "Hold with dancer posture",
            "rest": "30s",
            "notes": "Imagine lengthening through the crown. Core pulled in.",
            "imageUrl": "/exercises/plank/0.jpg"
          }
        ]
      }
    ]
  },

  "day_2_core_upper_body": {
    "focus": "Core & Upper Body Control",
    "goal": "Build the center strength and upper body control for partnering and port de bras",
    "sections": [
