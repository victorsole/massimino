{
  "program": "Hyrox Training Program",
  "program_id": "hyrox-training",
  "author": "HYROX",
  "author_url": "https://hyrox.com/",
  "version": "1.0",
  "duration_weeks": 8,
  "session_duration": {
    "min_minutes": 45,
    "max_minutes": 75
  },
  "frequency_per_week": 4,
  "level": "Intermediate to Advanced",
  "goal": {
    "outcome_goal": "Complete a simulated Hyrox race with improved times across all 8 stations",
    "progress_goals": [
      "Build running endurance for 8km total distance",
      "Develop functional strength for all 8 Hyrox stations",
      "Improve transitions between running and functional exercises",
      "Increase grip strength and muscular endurance"
    ]
  },

  "program_philosophy": {
    "core_principles": [
      "Train the 8 Hyrox stations with race-specific intensity",
      "Build running capacity to sustain 1km efforts between stations",
      "Develop mental toughness for the unique demands of fitness racing",
      "Practice transitions to minimize time lost between activities"
    ],
    "training_approach": "Hyrox combines 8km of running with 8 functional workout stations: SkiErg, Sled Push, Sled Pull, Burpee Broad Jumps, Rowing, Farmers Carry, Sandbag Lunges, and Wall Balls. This program prepares you for the unique demands of hybrid endurance-strength racing.",
    "differentiator": "Race-specific training that mirrors the exact Hyrox format while building progressive capacity"
  },

  "equipment": {
    "required": ["skierg", "rowing_machine", "sled", "kettlebells", "sandbag", "medicine_ball"],
    "recommended": ["heart_rate_monitor", "gym_timer"],
    "optional": ["weighted_vest", "battle_ropes"]
  },

  "hyrox_race_format": {
    "total_running": "8km (8 x 1km runs)",
    "stations": [
      {"station": 1, "exercise": "SkiErg", "distance_reps": "1000m"},
      {"station": 2, "exercise": "Sled Push", "distance_reps": "50m"},
      {"station": 3, "exercise": "Sled Pull", "distance_reps": "50m"},
      {"station": 4, "exercise": "Burpee Broad Jumps", "distance_reps": "80m"},
      {"station": 5, "exercise": "Rowing", "distance_reps": "1000m"},
      {"station": 6, "exercise": "Farmers Carry", "distance_reps": "200m"},
      {"station": 7, "exercise": "Sandbag Lunges", "distance_reps": "100m"},
      {"station": 8, "exercise": "Wall Balls", "distance_reps": "100 reps"}
    ],
    "division_weights": {
      "open_male": {"sled_push": "152kg", "sled_pull": "103kg", "farmers_carry": "2x24kg", "sandbag": "20kg", "wall_ball": "6kg"},
      "open_female": {"sled_push": "102kg", "sled_pull": "78kg", "farmers_carry": "2x16kg", "sandbag": "10kg", "wall_ball": "4kg"},
      "pro_male": {"sled_push": "202kg", "sled_pull": "153kg", "farmers_carry": "2x32kg", "sandbag": "30kg", "wall_ball": "9kg"},
      "pro_female": {"sled_push": "152kg", "sled_pull": "103kg", "farmers_carry": "2x24kg", "sandbag": "20kg", "wall_ball": "6kg"}
