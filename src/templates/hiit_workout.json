{
  "program": "HIIT Workout",
  "program_id": "hiit-workout",
  "author": "Victor Sole Ferioli",
  "version": "1.0",
  "duration_weeks": 4,
  "session_duration": {
    "min_minutes": 20,
    "max_minutes": 35
  },
  "frequency_per_week": 3,
  "level": "Intermediate",
  "goal": {
    "outcome_goal": "Improve cardiovascular capacity and metabolic conditioning through high-intensity intervals",
    "progress_goals": [
      "Increase work capacity by completing more rounds in the same time",
      "Reduce recovery time between intervals by 20%",
      "Improve resting heart rate recovery by end of program"
    ]
  },

  "program_philosophy": {
    "core_principles": [
      "Maximum effort during work intervals (85-95% max heart rate)",
      "Active or complete rest during recovery periods",
      "Progressive overload through reduced rest or increased work duration",
      "Full body engagement for maximum caloric burn"
    ],
    "training_approach": "HIIT alternates between short bursts of intense exercise and recovery periods, maximizing fat oxidation and improving both aerobic and anaerobic fitness.",
    "differentiator": "Time-efficient workouts that deliver results in under 30 minutes"
  },

  "equipment": {
    "required": ["bodyweight"],
    "recommended": ["timer", "heart_rate_monitor"],
    "optional": ["kettlebell", "jump_rope", "medicine_ball"]
  },

