  "program_id": "post-christmas-rentree",
  "target_audience": "Anyone returning to fitness after the holiday season, new year resolutioners, those who took a break during Christmas festivities",
  "duration_weeks": 6,
  "session_duration_minutes": {
    "min": 45,
    "max": 60
  },
  "frequency_per_week": 4,
  "rest_days_per_week": 3,
  "level": "Beginner to Intermediate",
  "programType": "LIFESTYLE",
  "settings": ["Gym or Home with basic equipment", "Minimal equipment version available"],
  "equipment": {
    "required": ["Dumbbells", "Bench or sturdy surface", "Pull-up bar or resistance bands"],
    "recommended": ["Barbell", "Cable machine", "Rowing machine or cardio equipment"],
    "optional": ["Kettlebell", "Medicine ball", "Jump rope"]
  },

  "program_philosophy": {
    "origin": "Let's be honest - the holidays hit us all. Between Christmas dinners, New Year's celebrations, and endless festive treats, most of us enter January carrying a few extra kilos and feeling sluggish. This program is designed specifically for the 'rentree' - the return to fitness after holiday indulgence.",
    "core_message": "\"You don't need to punish yourself for enjoying the holidays. You need a smart, progressive return to training that rebuilds your fitness without burning you out or causing injury.\"",
    "core_principles": [
      "Progressive return - start easier than you think you need",
      "Full body metabolic training for maximum calorie burn",
      "Rebuild strength foundation before increasing intensity",
      "Sustainable approach - no crash diets or extreme workouts",
      "Build habits that last beyond January"
    ]
  },

  "realistic_expectations": {
    "what_this_program_will_do": [
      "Help you lose 2-4kg of holiday weight safely",
      "Rebuild workout capacity and endurance",
      "Re-establish healthy training habits",
      "Improve energy levels and mood",
      "Build a foundation for more advanced training"
    ],
    "what_this_program_will_not_do": [
      "Give you a six-pack in 6 weeks (that takes longer)",
      "Undo years of inactivity in one month",
      "Replace proper nutrition (you still need to eat well)",
      "Work if you skip sessions"
    ],
    "honest_truth": "The holiday weight came from 2-3 weeks of excess. It will take 4-6 weeks of consistent effort to reverse. Be patient with yourself.",
    "timeline": {
      "week_1": "Getting back in the groove - expect soreness and fatigue",
      "weeks_2_3": "Building momentum - workouts feel more natural",
      "weeks_4_5": "Seeing results - clothes fit better, energy improves",
      "week_6": "New baseline - ready for more advanced programming"
    }
  },

  "training_schedule": {
    "day_1": "Full Body Metabolic A",
    "day_2": "Rest / Light Walk",
    "day_3": "Upper Body & Core",
    "day_4": "Rest / Active Recovery",
    "day_5": "Full Body Metabolic B",
    "day_6": "Lower Body & Cardio",
    "day_7": "Rest"
  },

  "phase_structure": [
    {
      "phase": "Re-Entry",
      "weeks": [1, 2],
      "focus": "Ease back into training, rebuild movement patterns",
      "intensity": "Moderate - stop 2-3 reps before failure",
      "cardio": "Low intensity steady state (LISS) - walks, light cycling",
      "rep_range": "12-15",
      "sets": "3",
      "rest_seconds": "60-90",
      "key_principle": "Rebuild workout capacity safely"
    },
    {
      "phase": "Build Up",
      "weeks": [3, 4],
      "focus": "Increase intensity, add complexity",
      "intensity": "Moderate-High - approach failure on final sets",
      "cardio": "Mix of LISS and intervals",
      "rep_range": "10-12",
      "sets": "4",
      "rest_seconds": "60-90",
      "key_principle": "Progressive overload"
    },
    {
      "phase": "Push Through",
      "weeks": [5, 6],
      "focus": "Maximum effort, fat loss focus",
      "intensity": "High - train to failure on compound movements",
      "cardio": "More HIIT, metabolic circuits",
      "rep_range": "8-12",
      "sets": "4",
      "rest_seconds": "60-90",
      "key_principle": "Maximum metabolic output"
    }
  ],

