  "nutrition_guidelines": {
    "general_approach": "You don't need to diet extremely. Focus on returning to normal, healthy eating patterns.",
    "key_principles": [
      "Protein at every meal (palm-sized portion)",
      "Fill half your plate with vegetables",
      "Reduce (don't eliminate) processed foods and sweets",
      "Drink water instead of sugary drinks",
      "Don't skip meals - leads to overeating later"
    ],
    "holiday_damage_control": [
      "The weight gain is mostly water retention and glycogen",
      "Reducing carbs slightly will help shed water weight quickly",
      "Don't crash diet - this leads to muscle loss",
      "Aim for 0.5-1kg loss per week maximum"
    ],
    "pre_workout": "Light carbs and protein 1-2 hours before",
    "post_workout": "Protein shake or meal within an hour",
    "alcohol": "Limit to weekends only during this program for best results"
  },

  "mindset_tips": {
    "week_1_motivation": "The hardest part is starting. You've already done that by beginning this program.",
    "dealing_with_soreness": "You WILL be sore. This is normal after time off. Move gently on rest days.",
    "comparison_trap": "Don't compare yourself to pre-holiday fitness. Start fresh.",
    "consistency_over_intensity": "Showing up 4 times per week beats going hard once and burning out.",
    "celebrate_small_wins": [
      "Completed a full week? Win.",
      "Lifted more than last session? Win.",
      "Chose gym over couch? Win."
    ]
  },

  "progression_guidelines": {
    "weeks_1_2": "Focus on form and consistency. Weight should feel moderate.",
    "weeks_3_4": "Increase weights by 5-10% when you complete all reps with good form.",
    "weeks_5_6": "Push harder. Final sets should approach failure.",
    "after_6_weeks": "You're ready for more advanced programming. Consider a strength-focused or physique-focused program."
  },

